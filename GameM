def PC(card):
    return card.CardType
def Value_Check(values):
    #Value = card Values = List
    count = 0
    for value in values:
        if value == 3:
            return count
        count +=1
    count = 0
    for value in values:
        if value == 2:
            return count
        count +=1
    count = 0
    for value in values:
        if value == 1:
            return count
        count +=1
    return False
